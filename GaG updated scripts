-- Combined Script: Your Script + Limit Hub

-- Load your custom script (Pastefy or GitHub)
local success1, err1 = pcall(function()
    loadstring(game:HttpGet("https://pastefy.app/TytbwluX/raw", true))()
end)
if not success1 then
    warn("[ERROR] Failed to load your script:", err1)
end

-- Load Limit Hub
local success2, err2 = pcall(function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/FakeModz/LimitHub/refs/heads/main/LimitHub_Loader.lua", true))()
end)
if not success2 then
    warn("[ERROR] Failed to load Limit Hub:", err2)
end
